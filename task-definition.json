{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "containerDefinitions": [
        {
            "name": "studymama-flutter",
            "image": "336218866683.dkr.ecr.ap-southeast-1.amazonaws.com/studymama-flutter:latest",
            "memoryReservation": "1024",
            "resourceRequirements": null,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": "4040",
                    "protocol": "tcp"
                }
            ],
            "environment": null,
            "environmentFiles": [],
            "secrets": null,
            "mountPoints": null,
            "volumesFrom": null,
            "hostname": null,
            "user": null,
            "workingDirectory": null,
            "extraHosts": null,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/studymama-flutter",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "ulimits": null,
            "dockerLabels": null,
            "dependsOn": null,
            "healthCheck": {
                "command": [
                    "CMD-SHELL curl --fail http://localhost:4040/ || exit 1"
                ],
                "interval": "30",
                "timeout": "5",
                "startPeriod": "300",
                "retries": "10"
            },
            "repositoryCredentials": {
                "credentialsParameter": ""
            }
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "1024",
    "cpu": "512",
    "executionRoleArn": "arn:aws:iam::336218866683:role/ecsTaskExecutionRole",
    "family": "studymama-flutter",
    "taskRoleArn": "arn:aws:iam::336218866683:role/ecsTaskExecutionRole",
    "tags": []
}
